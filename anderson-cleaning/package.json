{
  "name": "anderson-cleaning",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "echo 'Tests configured in CI/CD pipeline' && exit 0",
    "test:visual": "playwright test visual-regression",
    "test:visual:update": "playwright test visual-regression --update-snapshots",
    "test:visual:ui": "playwright test visual-regression --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "check:links": "BASE_URL=${BASE_URL:-https://andersoncleaning.com} tsx scripts/check-internal-links.ts",
    "check:links:local": "BASE_URL=http://localhost:3000 tsx scripts/check-internal-links.ts",
    "check:links:verbose": "BASE_URL=http://localhost:3000 tsx scripts/check-internal-links.ts --verbose",
    "check:i18n": "tsx scripts/check-i18n-coverage.ts",
    "check:i18n:unused": "tsx scripts/check-i18n-coverage.ts --unused",
    "check:i18n:verbose": "tsx scripts/check-i18n-coverage.ts --verbose",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/genai": "^1.3.0",
    "@headlessui/react": "^2.0.0",
    "@hookform/resolvers": "^3.3.0",
    "@portabletext/react": "^5.0.0",
    "@sanity/client": "^6.29.1",
    "@sanity/icons": "^3.4.0",
    "@sanity/image-url": "^1.0.2",
    "@sanity/types": "^3.57.4",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^4.15.0",
    "@sentry/nextjs": "^8.0.0",
    "@vercel/og": "^0.6.0",
    "clsx": "^2.1.0",
    "critters": "^0.0.23",
    "framer-motion": "^12.15.0",
    "i18next": "^25.6.2",
    "lucide-react": "^0.436.0",
    "next": "^14.2.0",
    "next-intl": "^3.15.0",
    "next-sanity": "^11.6.6",
    "next-seo": "^6.8.0",
    "next-sitemap": "^4.2.3",
    "react": "^18.3.0",
    "react-compare-slider": "^3.1.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.0",
    "react-i18next": "^16.3.2",
    "react-is": "^19.2.0",
    "resend": "^3.2.0",
    "sanity": "^4.15.0",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^2.3.0",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^10.1.8",
    "file-type": "^16.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0"
  }
